#include <stdio.h>
#include <math.h>

int main(){
  int x1, x2, x3, y1, y2, y3;
  int l1, l2, l3;
  printf("x1 y1: ");
  scanf("%i %i", &x1, &y1);
  printf("x2 y2: ");
  scanf("%i %i", &x2, &y2);
  printf("x3 y3: ");
  scanf("%i %i", &x3, &y3);

  l1 = pow(x2 - x1, 2) + pow(y2 - y1, 2);
  l2 = pow(x3 - x2, 2) + pow(y3 - y2, 2);
  l3 = pow(x3 - x1, 2) + pow(y3 - y1, 2);

  if((l1 > l2 && l1 > l3 && l1 == l2 + l3) || (l2 > l1 && l2 > l3 && l2 == l1 + l3) || (l3 > l1 && l3 > l2 && l3 == l2 + l1))
    printf("Треугольник прямоугольный\n");
  else 
    printf("Треугольник не прямоугольный\n");
  return 0;
}
